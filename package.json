    "browserify": "~12.0.0",
    "del": "~2.0.2",
    "envify": "~3.4.0",
    "foldero": "~0.1.1",
    "glob": "~5.0.15",
    "gulp": "~3.9.0",
    "gulp-changed": "~1.3.0",
    "gulp-compass": "^2.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-data": "~1.2.0",
    "gulp-eslint": "~1.0.0",
    "gulp-filter": "^3.0.1",
    "gulp-if": "~2.0.0",
    "gulp-jade": "~1.1.0",
    "gulp-load-plugins": "~1.0.0",
    "gulp-main-bower-files": "^1.2.5",
    "gulp-minify-css": "^1.2.1",
    "gulp-newer": "~1.0.0",
    "gulp-plumber": "~1.0.1",
    "gulp-postcss": "~6.0.1",
    "gulp-rename": "~1.2.2",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.4.2",
    "gulp-util": "~3.0.7",
    "jade": "~1.11.0",
    "lodash": "~3.10.1",
    "minimist": "~1.2.0",
    "rimraf": "~2.4.3",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "wrench": "~1.5.8"
  },
  "scripts": {
    "clean-deps": "rimraf node_modules"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "2019",
    "baseUrl": "./",
    "directories": {
      "source": "source",
      "destination": "dist",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "jade": "_jade",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_css",
      "scripts": "_js",
      "data": "_data"
    },
    "entries": {
      "js": "**.js",
      "css": "**.{sass,scss}"
    }
  }
}
